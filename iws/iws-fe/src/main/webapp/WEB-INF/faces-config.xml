<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <application>
        <locale-config>
            <default-locale>en</default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>net.iaeste.iws.fe.Messages</base-name>
            <var>amsg</var>
        </resource-bundle>
    </application>

    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.MenuButtonRenderer</renderer-type>
            <renderer-class>net.iaeste.iws.fe.primefaces.component.menubutton.IWMenuButtonRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.OutputLabelRenderer</renderer-type>
            <renderer-class>net.iaeste.iws.fe.primefaces.component.outputlabel.IWOutputLabelRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>